	"keywords" : [
		"Audio",
		"Metadata",
		"Mp3"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Oliver Kotte",
			"email" : "oliver.kotte@stinger-soft.net"
		}, {
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\MediaParsingBundle\\" : ""
		}
	},
	"license" : "MIT",
	"require" : {
		"php" : ">=5.5",
		"symfony/framework-bundle" : "^2.7",
		"phansys/getid3" : "2.1.*@dev"
	},
	"require-dev" : {
